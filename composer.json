{
    "name": "falgunphp/fancyerror",
    "type": "library",
    "homepage": "https://github.com/falgun/fancyerror",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "S.M. Ataur Rahman",
            "email": "dev.ataur.rahman@gmail.com",
            "homepage": "https://ataur.net/"
        }
    ],
    "autoload": {
        "psr-4": {
            "Falgun\\FancyError\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Falgun\\FancyError\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.4",
        "falgunphp/application": "dev-master",
        "falgunphp/fountain": "dev-master",
        "falgunphp/http": "^0.0.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.4",
        "sempro/phpunit-pretty-print": "^1.3",
        "vimeo/psalm": "^4.0"
    },
    "scripts": {
        "phpunit": "phpunit --verbose --colors=always"
    },
    "config": {
        "sort-packages": true
    }
}
